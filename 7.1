import java.util.Scanner;

public class FuelConsumption {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        double kilometers, liters, consumption;

        System.out.print("Enter the number of kilometers traveled: ");
        kilometers = scanner.nextDouble();

        System.out.print("Enter the number of liters of fuel consumed: ");
        liters = scanner.nextDouble();

        if (liters > 0) {
            consumption = kilometers / liters;
            System.out.printf("The car's fuel consumption is %.2f kilometers per liter.%n", consumption);
        } else {
            System.out.println("Cannot divide by zero. Please check the amount of fuel entered.");
        }

        scanner.close();
    }
}
