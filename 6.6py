def print_table(number):
    print(f"\nMultiplication Table of {number}:")
    print("-" * 30)
    for i in range(1, 11):
        result = number * i
        print(f"{number} * {i} = {result}")
    print("-" * 30)
try:
    number = int(input("Enter a number to see its multiplication table: "))
    print_table(number)
except ValueError:
    print("Invalid input. Please enter a valid integer.")   
